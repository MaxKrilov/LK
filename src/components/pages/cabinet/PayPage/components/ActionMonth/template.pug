// Action Month
div
  er-row.align-items-center(:class="`${pre}__service__data`")
    div.ml-16.mr-8(:class="`${pre}__service__data__dm`")
      | {{month}}
    div(:class="`${pre}__service__data__yy`")
      | '17
  div(style="height: 60px")
  er-row(no-gutters)
    er-flex.mb-16(xs12)
      Operations(index=0)
      Operations(index=1)
      Operations(index=2)
      Operations(index=3)
      Operations(index=4)
