// Action Month

div
  div.d--flex.align-items-center(
    :class="`${pre}__service__date`"
  )
    div.mr-8(
      :class="`${pre}__service__date__dm`"
    ) {{ month }}
    div(:class="`${pre}__service__date__yy`")
      | '{{ year }}
  div(:class="`${pre}__height`")
  er-row(no-gutters)
    er-flex.mb-16(xs12)
      Operations(
        v-for="(subItem, index) in item"
        :key="index"
        :item="subItem"
      )

