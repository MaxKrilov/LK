er-row(:class="`${pre}`")
  span(:class="`${pre}__value`") {{value}}
  er-row(xs12 sm6 md6 lg4 :class="`${pre}__actions`")
    div(
      :class="`${pre}__delete`"
      @click="onRemove()")
      er-icon(:class="`${pre}__icon`" name="close")
    div(
      :class="`${pre}__preference`"
      @click="onPrefer()")
      er-icon(:class="`${pre}__icon`" name="pin")
